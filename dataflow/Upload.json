{
	"name": "Upload",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CompaniesDataIN",
						"type": "DatasetReference"
					},
					"name": "Companies"
				},
				{
					"dataset": {
						"referenceName": "BranchesIN",
						"type": "DatasetReference"
					},
					"name": "Branches"
				},
				{
					"dataset": {
						"referenceName": "ManagersIN",
						"type": "DatasetReference"
					},
					"name": "Managers"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "CompaniesCosmos",
						"type": "DatasetReference"
					},
					"name": "CompaniesCosmos"
				},
				{
					"dataset": {
						"referenceName": "BranchesCosmos",
						"type": "DatasetReference"
					},
					"name": "BranchesCosmos"
				},
				{
					"dataset": {
						"referenceName": "ManagersCosmos",
						"type": "DatasetReference"
					},
					"name": "ManagerCosmos"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> Companies",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> Branches",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> Managers",
				"Companies sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     recreate:true,",
				"     format: 'document',",
				"     partitionKey: ['/id'],",
				"     throughput: 1000,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> CompaniesCosmos",
				"Branches sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     recreate:true,",
				"     format: 'document',",
				"     partitionKey: ['/id'],",
				"     throughput: 1000,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> BranchesCosmos",
				"Managers sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     recreate:true,",
				"     format: 'document',",
				"     partitionKey: ['/contactId'],",
				"     throughput: 1000,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> ManagerCosmos"
			]
		}
	}
}