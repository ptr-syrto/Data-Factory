{
	"name": "Test1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "FullData",
						"type": "DatasetReference"
					},
					"name": "StagingData"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "CompaniesCosmos",
						"type": "DatasetReference"
					},
					"name": "Upload"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     format: 'parquet') ~> StagingData",
				"StagingData sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     recreate:true,",
				"     format: 'document',",
				"     partitionKey: ['id'],",
				"     throughput: 500,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Upload"
			]
		}
	}
}